<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_checked="true" android:drawable="@drawable/btn_check_on_holo_dark" />
	<item android:state_pressed="true" android:state_checked="true" android:drawable="@drawable/btn_check_on_pressed_holo_dark" />
	<item android:state_pressed="true" android:state_checked="false" android:drawable="@drawable/btn_check_off_pressed_holo_dark" />
	<item android:drawable="@drawable/btn_check_off_normal_holo_dark" android:state_checked="false" />

</selector>

